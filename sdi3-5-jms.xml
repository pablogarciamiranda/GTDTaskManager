<?xml version="1.0" encoding="UTF-8"?>
<messaging-deployment xmlns="urn:jboss:messaging-deployment:1.0">
	<hornetq-server>
		<jms-destinations>
			<jms-queue name="MessagesQueue">
				<entry name="queue/MessagesQueue" />
				<entry name="java:jboss/exported/jms/queue/MessagesQueue" />
				<durable>true</durable>
			</jms-queue>
			<jms-queue name="InvalidMessagesQueue">
				<entry name="queue/InvalidMessagesQueue" />
				<entry name="java:jboss/exported/jms/queue/InvalidMessagesQueue" />
				<durable>true</durable>
			</jms-queue>
		</jms-destinations>
	</hornetq-server>
</messaging-deployment>